{
  "actions": {
    "zombies": {
      "description": "zombies",
      "modifies": true
    }
  },
  "credentials": {
    "my-first-cred": {
      "env": "MY_FIRST_CRED",
      "required": true
    },
    "my-second-cred": {
      "description": "My second cred",
      "path": "/path/to/my-second-cred",
      "required": true
    }
  },
  "custom": {
    "sh.porter": {
      "manifest": "",
      "manifestDigest": "a5a44a16ead32ade9ce62a6fc8ba5743bdb5e16713131dfb5d158d6d8928892e",
      "mixins": {
        "exec": {}
      }
    }
  },
  "definitions": {
    "kubeconfig-output": {
      "contentEncoding": "base64",
      "type": "string"
    },
    "my-first-output-output": {
      "type": "string",
      "writeOnly": true
    },
    "my-first-param-parameter": {
      "default": 9,
      "type": "integer"
    },
    "my-second-output-output": {
      "description": "My second output",
      "type": "boolean"
    },
    "my-second-param-parameter": {
      "default": "spring-music-demo",
      "description": "My second parameter",
      "type": "string",
      "writeOnly": true
    },
    "porter-debug-parameter": {
      "$id":"https://getporter.org/generated-bundle/#porter-debug",
      "$comment":"porter-internal",
      "default": false,
      "description": "Print debug information from Porter when executing the bundle",
      "type": "boolean"
    }
  },
  "description": "A bundle with a custom action",
  "images": {
    "something": {
      "description": "an image",
      "image": "getporter/boo:latest",
      "imageType": "docker"
    }
  },
  "invocationImages": [
    {
      "image": "getporter/porter-hello-installer:0.1.0",
      "imageType": "docker"
    }
  ],
  "name": "porter-hello",
  "outputs": {
    "kubeconfig": {
      "definition": "kubeconfig-output",
      "path": "/cnab/app/outputs/kubeconfig"
    },
    "my-first-output": {
      "applyTo": [
        "install",
        "upgrade"
      ],
      "definition": "my-first-output-output",
      "path": "/cnab/app/outputs/my-first-output"
    },
    "my-second-output": {
      "definition": "my-second-output-output",
      "description": "My second output",
      "path": "/cnab/app/outputs/my-second-output"
    }
  },
  "parameters": {
    "my-first-param": {
      "applyTo": [
        "install"
      ],
      "definition": "my-first-param-parameter",
      "destination": {
        "env": "MY_FIRST_PARAM"
      }
    },
    "my-second-param": {
      "definition": "my-second-param-parameter",
      "description": "My second parameter",
      "destination": {
        "path": "/path/to/my-second-param"
      }
    },
    "porter-debug": {
      "definition": "porter-debug-parameter",
      "description": "Print debug information from Porter when executing the bundle",
      "destination": {
        "env": "PORTER_DEBUG"
      }
    }
  },
  "schemaVersion": "v1.0.0",
  "version": "0.1.0"
}